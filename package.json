{
  "name": "meta",
  "version": "1.0.0",
  "description": "Meta info service",
  "main": "index.js",
  "scripts": {
    "import:gutenberg": "ts-node-dev src/bin/gutenberg.ts",
    "build": "tsc",
    "test": "ava-ts",
    "test:watch": "npm test -- --watch",
    "migrations": "DEBUG=sequelize:sql* dotenv sequelize db:migrate",
    "migrations:undo": "DEBUG=sequelize:sql* dotenv sequelize db:migrate:undo",
    "migrations:generate": "sequelize migration:generate --name",
    "proc": "NODE_PATH=. pm2 start --no-daemon",
    "schema:publish": "apollo service:push --key service:postgres-day:2p5YYz-CY_kmCf_HYUJLIQ --endpoint \"http://localhost:8081/api\"",
    "schema:check": "apollo service:check --key service:postgres-day:2p5YYz-CY_kmCf_HYUJLIQ --endpoint \"http://localhost:8081/api\"",
    "schema:introspect": "curl -X POST -d \"@fixtures/introspect.json\" --header \"Content-Type: application/json\" http://localhost:8081 /api",
    "seed": "dotenv sequelize db:seed:all",
    "start": "pm2 start --no-daemon",
    "start:dev": "nodemon",
    "gen:apq": "persistgraphql src/interfaces/graphql/persistedQueries/ src/interfaces/graphql/persistedQueries/config.json",
    "lint": "tslint --project ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hyped-text/meta.git"
  },
  "keywords": [
    "full-text",
    "books",
    "ssr"
  ],
  "author": "shonie",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hyped-text/meta/issues"
  },
  "homepage": "https://github.com/hyped-text/meta#readme",
  "dependencies": {
    "apollo-datasource": "^0.2.1",
    "apollo-server-koa": "^2.3.1",
    "apollo-server-redis": "0.1.0-rc.11",
    "dotenv": "^6.1.0",
    "dotenv-cli": "^1.4.0",
    "graphql": "^14.0.2",
    "hyped-logger": "git+https://github.com/hyped-text/hyped-logger.git",
    "isomorphic-fetch": "^2.2.1",
    "koa": "^2.6.2",
    "koa-bodyparser": "^4.2.1",
    "koa-compose": "^4.1.0",
    "koa-logger-winston": "0.0.2",
    "koa-router": "^7.4.0",
    "nconf": "^0.10.0",
    "parse-database-url": "^0.3.0",
    "pm2": "^3.2.4",
    "ramda": "^0.26.1",
    "sparql-client": "^0.2.0"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.4.32",
    "@types/dotenv": "^6.1.0",
    "@types/graphql": "^14.0.6",
    "@types/isomorphic-fetch": "0.0.34",
    "@types/mkdirp": "^0.5.2",
    "@types/nconf": "^0.10.0",
    "@types/node": "^10.12.21",
    "@types/ramda": "^0.25.48",
    "@types/redis": "^2.8.10",
    "@types/rimraf": "^2.0.2",
    "@types/sequelize": "^4.27.35",
    "@types/sinon": "^7.0.5",
    "@types/tar-fs": "^1.16.1",
    "apollo": "^2.3.0",
    "apollo-server-testing": "^2.3.1",
    "ava": "^1.2.1",
    "ava-ts": "^0.25.2",
    "nodemon": "^1.18.10",
    "prettier": "^1.15.3",
    "sinon": "^7.2.3",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.1"
  }
}
